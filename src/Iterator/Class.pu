@startuml

class Iterator {
  +hasNext()
  +next()
}

class Book {
  -name
  +getName()
}

class BookShelf {
  -last
  -book
  +getBookAt()
  +appendBook()
  +getLength()
  +iterator()
}

class BookShelfIterator{
  - bookShelf
  - index
  +hasNext()
  +next()
}

Iterator <.up.BookShelfIterator
BookShelf o-> Book
BookShelf <-down-o BookShelfIterator


@enduml
